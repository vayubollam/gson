<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@id/home_tab">

    <keep_state_fragment
        android:id="@+id/home_tab"
        android:name="suncor.com.android.ui.main.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home_guest">
        <argument
            android:name="root"
            android:defaultValue="true"
            app:argType="boolean" />
        <action
            android:id="@+id/action_home_tab_to_cardsDetailsFragment"
            app:destination="@id/cardsDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_home_tab_to_rewardsDiscoveryFragment"
            app:destination="@id/rewardsDiscoveryFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/rewards_tab"
        android:name="suncor.com.android.ui.main.rewards.RewardsGuestFragment"
        android:label="RewardsFragment"
        tools:layout="@layout/fragment_rewards">
        <argument
            android:name="root"
            android:defaultValue="true"
            app:argType="boolean" />
    </keep_state_fragment>

    <keep_state_fragment
        android:id="@+id/stations_tab"
        android:name="suncor.com.android.ui.main.stationlocator.StationsFragment"
        android:label="StationsFragment"
        tools:layout="@layout/fragment_stations">
        <argument
            android:name="root"
            android:defaultValue="true"
            app:argType="boolean" />
        <action
            android:id="@+id/action_stations_tab_to_filtersFragment"
            app:destination="@id/filtersFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_stations_tab_to_searchFragment"
            app:destination="@id/searchFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_stations_tab_to_favouritesFragment"
            app:destination="@id/favouritesFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/rewards_signedin_tab"
        android:name="suncor.com.android.ui.main.rewards.RewardsSignedInFragment"
        android:label="RewardsSignedInFragment"
        tools:layout="@layout/fragment_rewards_signedin">
        <argument
            android:name="root"
            android:defaultValue="true"
            app:argType="boolean" />
        <action
            android:id="@+id/action_rewards_signedin_tab_to_rewardsDetailsFragment"
            app:destination="@id/rewardsDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_rewards_signedin_tab_to_rewardsDiscoveryFragment"
            app:destination="@id/rewardsDiscoveryFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_rewards_signedin_tab_to_merchantDetailsFragment"
            app:destination="@id/merchantDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
    </keep_state_fragment>

    <keep_state_fragment
        android:id="@+id/cards_tab"
        android:name="suncor.com.android.ui.main.cards.list.CardsFragment"
        android:label="CardsFragment"
        tools:layout="@layout/fragment_stations">
        <argument
            android:name="root"
            android:defaultValue="true"
            app:argType="boolean" />
        <action
            android:id="@+id/action_cards_tab_to_cardsDetailsFragment"
            app:destination="@id/cardsDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_cards_tab_to_addCardFragment"
            app:destination="@id/addCardFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />

    </keep_state_fragment>

    <keep_state_fragment
        android:id="@+id/profile_tab"
        android:name="suncor.com.android.ui.main.profile.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profile_tab_to_FAQFragment"
            app:destination="@id/FAQFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profile_tab_to_transactionsFragment"
            app:destination="@+id/transactionsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profile_tab_to_aboutFragment"
            app:destination="@+id/aboutFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profile_tab_to_securityQuestionValidationFragment2"
            app:destination="@id/securityQuestionValidationFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/dummy"
            app:launchSingleTop="false"
            app:popExitAnim="@anim/fade_out"
            app:popUpTo="@+id/profile_tab" />
        <action
            android:id="@+id/action_profile_tab_to_preferencesFragment"
            app:destination="@id/preferencesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profile_tab_to_personalInfoFragment"
            app:destination="@id/personalInfoFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profile_tab_to_addressFragment"
            app:destination="@id/addressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/filtersFragment"
        android:name="suncor.com.android.ui.main.stationlocator.FiltersFragment"
        android:label="FiltersFragment" />
    <keep_state_fragment
        android:id="@+id/searchFragment"
        android:name="suncor.com.android.ui.main.stationlocator.search.SearchFragment"
        android:label="SearchFragment" />
    <keep_state_fragment
        android:id="@+id/favouritesFragment"
        android:name="suncor.com.android.ui.main.stationlocator.favourites.FavouritesFragment"
        android:label="FavouritesFragment" />
    <keep_state_fragment
        android:id="@+id/FAQFragment"
        android:name="suncor.com.android.ui.main.profile.help.FAQFragment"
        android:label="FAQFragment">
        <action
            android:id="@+id/action_FAQFragment_to_FAQResponse"
            app:destination="@id/FAQResponse"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/FAQResponse"
        android:name="suncor.com.android.ui.main.profile.help.FAQResponseFragment"
        android:label="FAQResponse" />
    <keep_state_fragment
        android:id="@+id/cardsDetailsFragment"
        android:name="suncor.com.android.ui.main.cards.details.CardsDetailsFragment"
        android:label="CardsDetailsFragment">
        <argument
            android:name="cardIndex"
            android:defaultValue="0"
            app:argType="integer" />
        <argument
            android:name="isCardFromProfile"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="isCardFromCarWash"
            android:defaultValue="false"
            app:argType="boolean" />
        <action
            android:id="@+id/action_cardsDetailsFragment_to_carWashActivationSecurityFragment"
            app:destination="@id/carWashActivationSecurityFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right"/>
    </keep_state_fragment>

    <keep_state_fragment
        android:id="@+id/addCardFragment"
        android:name="suncor.com.android.ui.main.cards.add.AddCardFragment"
        android:label="AddCardFragment"
        tools:layout="@layout/fragment_add_card" />
    <keep_state_fragment
        android:id="@+id/personalInfoFragment"
        android:name="suncor.com.android.ui.main.profile.info.PersonalInfoFragment"
        android:label="PersonalInfoFragment"
        tools:layout="@layout/fragment_personal_info" />
    <keep_state_fragment
        android:id="@+id/transactionsFragment"
        android:name="suncor.com.android.ui.main.profile.transcations.TransactionsFragment"
        android:label="TransactionsFragment">
        <action
            android:id="@+id/action_transactionsFragment_to_transactionDetailFragment"
            app:destination="@id/transactionDetailFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/transactionsFragment" />
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/preferencesFragment"
        android:name="suncor.com.android.ui.main.profile.preferences.PreferencesFragment"
        android:label="PreferencesFragment" />
    <keep_state_fragment
        android:id="@+id/transactionDetailFragment"
        android:name="suncor.com.android.ui.main.profile.transcations.TransactionDetailFragment"
        android:label="TransactionDetailFragment">
        <argument
            android:name="transaction"
            app:argType="suncor.com.android.model.cards.Transaction"
            app:nullable="true" />
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/aboutFragment"
        android:name="suncor.com.android.ui.main.profile.about.AboutFragment"
        android:label="PersonalInfoFragment"
        tools:layout="@layout/fragment_about" />
    <keep_state_fragment
        android:id="@+id/rewardsDetailsFragment"
        android:name="suncor.com.android.ui.main.rewards.RewardsDetailsFragment"
        android:label="RewardsDetailsFragment">
        <argument
            android:name="reward"
            app:argType="suncor.com.android.ui.main.rewards.Reward" />

    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/rewardsDiscoveryFragment"
        android:name="suncor.com.android.ui.main.rewards.RewardsDiscoveryFragment"
        android:label="RewardsDiscoveryFragment" />
    <keep_state_fragment
        android:id="@+id/securityQuestionValidationFragment"
        android:name="suncor.com.android.ui.main.profile.securityquestion.SecurityQuestionValidationFragment"
        android:label="SecurityQuestionValidationFragment">
        <action
            android:id="@+id/action_securityQuestionValidationFragment_to_preferencesFragment2"
            app:destination="@id/preferencesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profile_tab" />
        <action
            android:id="@+id/action_securityQuestionValidationFragment_to_personalInfoFragment"
            app:destination="@id/personalInfoFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profile_tab" />
        <argument
            android:name="destinationFragment"
            app:argType="string" />
        <action
            android:id="@+id/action_securityQuestionValidationFragment_to_addressFragment"
            app:destination="@id/addressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profile_tab" />
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/addressFragment"
        android:name="suncor.com.android.ui.main.profile.address.AddressFragment"
        android:label="AddressFragment">
        <action
            android:id="@+id/action_addressFragment_to_provinceProfileFragment"
            app:destination="@id/provinceProfileFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/provinceProfileFragment"
        android:name="suncor.com.android.ui.main.profile.address.ProvinceProfileFragment"
        android:label="ProvinceProfileFragment" />
    <action
        android:id="@+id/action_global_home_tab"
        app:destination="@id/home_tab" />
    <keep_state_fragment
        android:id="@+id/merchantDetailsFragment"
        android:name="suncor.com.android.ui.main.rewards.MerchantDetailsFragment"
        android:label="MerchantDetailsFragment">
        <argument
            android:name="MerchantItem"
            app:argType="suncor.com.android.ui.main.rewards.MerchantItem" />
        <action
            android:id="@+id/action_merchantDetailsFragment_to_giftCardValueConfirmation"
            app:destination="@id/giftCardValueConfirmation"
            app:popUpTo="@+id/merchantDetailsFragment" />
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/giftCardValueConfirmation"
        android:name="suncor.com.android.ui.main.rewards.redeem.GiftCardValueConfirmationFragment"
        android:label="GiftCardValueConfirmation">
        <argument
            android:name="merchanItem"
            app:argType="suncor.com.android.ui.main.rewards.MerchantItem" />
        <action
            android:id="@+id/action_giftCardValueConfirmation_to_redeemReceiptFragment"
            app:destination="@id/redeemReceiptFragment"
            app:exitAnim="@anim/fade_out"
            app:popExitAnim="@anim/fade_out"
            app:popUpTo="@+id/rewards_signedin_tab" />
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/redeemReceiptFragment"
        android:name="suncor.com.android.ui.main.rewards.RedeemReceiptFragment"
        android:label="RedeemReceiptFragment">
        <argument
            android:name="orderResponse"
            app:argType="suncor.com.android.model.redeem.response.OrderResponse" />
    </keep_state_fragment>
    <action
        android:id="@+id/action_to_profile_tab"
        app:destination="@id/profile_tab"
        app:enterAnim="@anim/slide_in_right"
        app:exitAnim="@anim/dummy"
        app:popExitAnim="@anim/slide_out_right" />
    <action
        android:id="@+id/action_to_cardsDetailsFragment"
        app:destination="@id/cardsDetailsFragment"
        app:enterAnim="@anim/slide_up"
        app:exitAnim="@anim/dummy"
        app:popExitAnim="@anim/slide_down">
        <argument
            android:name="isCardFromProfile"
            android:defaultValue="true" />
    </action>
    <action
        android:id="@+id/action_to_carWashFragment"
        app:destination="@id/carWashCardFragment"
        app:enterAnim="@anim/slide_in_right"
        app:exitAnim="@anim/dummy"
        app:popExitAnim="@anim/slide_out_right" />
    <keep_state_fragment
        android:id="@+id/carWashCardFragment"
        android:name="suncor.com.android.ui.main.carwash.CarWashCardFragment"
        android:label="CarWashCardFragment">
        <action
            android:id="@+id/action_carWashCardFragment_to_carWashPurchaseFragment"
            app:destination="@id/carWashPurchaseFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_carWashCardFragment_to_cardsDetailsFragment"
            app:destination="@id/cardsDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down"/>
    </keep_state_fragment>
    <keep_state_fragment
        android:id="@+id/carWashPurchaseFragment"
        android:name="suncor.com.android.ui.main.carwash.CarWashPurchaseFragment"
        android:label="CarWashPurchaseFragment" />
    <keep_state_fragment
        android:id="@+id/carWashActivationSecurityFragment"
        android:name="suncor.com.android.ui.main.carwash.CarWashActivationSecurityFragment"
        android:label="CarWashActivationSecurityFragment" />

</navigation>