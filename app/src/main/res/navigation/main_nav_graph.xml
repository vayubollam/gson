            android:id="@+id/action_profile_tab_to_accountDetailsFragment"
            app:destination="@id/accountDetailsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profile_tab" />


    </keep_state_fragment>

    <keep_state_fragment
        android:id="@+id/accountDetailsFragment"
        android:name="suncor.com.android.ui.main.profile.account.AccountDetailsFragment"
