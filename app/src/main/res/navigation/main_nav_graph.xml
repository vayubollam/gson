        <action
            android:id="@+id/action_more_e_gift_card_categories_to_merchantDetailsFragment"
            app:destination="@id/merchantDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
    </keep_state_fragment>

    <keep_state_fragment
        android:id="@+id/cards_tab"
        android:name="suncor.com.android.ui.main.wallet.cards.list.CardsFragment"
        android:label="CardsFragment"
        tools:layout="@layout/fragment_cards">
