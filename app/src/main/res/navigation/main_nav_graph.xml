            android:id="@+id/action_cards_tab_to_cardsDetailsFragment"
            app:destination="@id/cardsDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_cards_tab_to_addCardFragment"
            app:destination="@id/addCardFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_cards_tab_to_carWashPurchaseFragment"
            app:destination="@id/carWashPurchaseFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right">
            <argument
                android:name="isFromCardsTab"
                android:defaultValue="true"
                app:argType="boolean" />
        </action>

    </keep_state_fragment>

    <keep_state_fragment
        android:id="@+id/wallet_tab"
        android:name="suncor.com.android.ui.main.wallet.WalletFragment"
        android:label="WalletFragment"
        tools:layout="@layout/fragment_wallet">
        <argument
            android:name="root"
            android:defaultValue="true"
            app:argType="boolean" />
        <action
            android:id="@+id/action_cards_tab_to_cardsDetailsFragment"
            app:destination="@id/cardsDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_cards_tab_to_addCardFragment"
            app:destination="@id/addCardFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_cards_tab_to_carWashPurchaseFragment"
            app:destination="@id/carWashPurchaseFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right">
            <argument
                android:name="isFromCardsTab"
                android:defaultValue="true"
                app:argType="boolean" />
        </action>
        <action
            android:id="@+id/action_payments_tab_to_paymentsDetailsFragment"
            app:destination="@id/paymentsDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_payments_tab_to_addPaymentFragment"
            app:destination="@id/addPaymentFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_down" />

    </keep_state_fragment>

    <keep_state_fragment
        android:id="@+id/profile_tab"
        android:name="suncor.com.android.ui.main.profile.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profile_tab_to_FAQFragment"
            app:destination="@id/FAQFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profile_tab_to_transactionsFragment"
            app:destination="@+id/transactionsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profile_tab_to_aboutFragment"
            app:destination="@+id/aboutFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/dummy"
            app:popExitAnim="@anim/slide_out_right" />
