        android:label="FAQResponse" />
    <keep_state_fragment
        android:id="@+id/cardsDetailsFragment"
        android:name="suncor.com.android.ui.main.wallet.cards.details.CardsDetailsFragment"
        android:label="CardsDetailsFragment">
        <argument
            android:name="cardIndex"
            android:defaultValue="0"
            app:argType="integer" />
        <argument
            android:name="loadType"
            android:defaultValue="ALL"
            app:argType="suncor.com.android.ui.main.wallet.cards.CardsLoadType" />
